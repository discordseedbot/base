{
	"plugins": [],
	"recurseDepth": 10,
	"source": {
		"include": ["core"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"templates": {
		"cleverLinks": true,
		"monospaceLinks": false
	},
	"opts": {
		"recurse": true,
		"destination": "./docs/",
		"template": "./node_modules/tui-jsdoc-template"
	}
}